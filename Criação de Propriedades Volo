if (select count(*) from PROPRIEDADE_GRUPO where COD_GRUPO = 'WM') = 0
INSERT INTO PROPRIEDADE_GRUPO
SELECT 
'WM','WMS'

IF (SELECT COUNT(*) FROM TABELA_ADMINISTRACAO WHERE TABELA = 'FILIAIS') = 0
INSERT INTO TABELA_ADMINISTRACAO
SELECT TABELA = 'FILIAIS'

IF (SELECT COUNT(*) FROM PROPRIEDADE WHERE PROPRIEDADE = 'WMS01') = 0
INSERT INTO PROPRIEDADE
SELECT 
PROPRIEDADE = 'WMS01',
